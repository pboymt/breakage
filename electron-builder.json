{
  "productName": "Breakage",
  "appId": "icu.pboymt.${name}",
  "artifactName": "${name}-${platform}-${arch}-${version}.${ext}",
  "directories": {
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!*.code-workspace",
    "!LICENSE.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!angular.json",
    "!_config.yml",
    "!karma.conf.js",
    "!tsconfig.json",
    "!tsconfig.serve.json",
    "!tslint.json",
    "!Reference.md",
    "!.data/",
    "!.github/",
    "!.vscode/",
    "!test.js"
  ],
  "win": {
    "icon": "dist/assets/icons",
    "target": [
      "portable"
    ]
  },
  "mac": {
    "icon": "dist/assets/icons",
    "target": [
      "dmg"
    ]
  },
  "linux": {
    "icon": "dist/assets/icons",
    "target": [
      "AppImage"
    ]
  }
}